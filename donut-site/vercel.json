{
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://example-api.com/:1"
      }
    ],
    "headers": [
      {
        "source": "/fonts/(.*)",
        "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
      }
    ],
    "trailingSlash": "remove",
    "cleanUrls": true
}
  